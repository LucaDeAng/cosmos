{
  "name": "backend-system",
  "version": "1.0.0",
  "description": "Secure backend system with user management, authentication, and initiative tracking",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc && npm run copy-prompts",
    "copy-prompts": "xcopy /E /I /Y src\\agents\\prompts dist\\agents\\prompts >nul 2>&1 || cp -r src/agents/prompts dist/agents/prompts",
    "start": "node dist/index.js",
    "migrate": "node scripts/run-migration.js",
    "test": "jest --runInBand",
    "test:e2e": "jest __tests__/e2e/**/*.test.ts --runInBand",
    "test:battery": "npx ts-node __tests__/ingestion/battery/runBatteryTests.ts",
    "test:battery:quick": "npm run test:battery -- --quick",
    "test:battery:download": "npm run test:battery -- --download-only",
    "test:battery:verbose": "npm run test:battery -- --verbose",
    "test:battery:baseline": "npm run test:battery -- --save-baseline",
    "lint": "eslint . --ext .ts --ignore-path .eslintignore"
  },
  "keywords": [
    "backend",
    "api",
    "authentication",
    "supabase"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@azure/ai-form-recognizer": "^5.1.0",
    "@langchain/community": "^1.1.1",
    "@langchain/core": "^1.1.0",
    "@langchain/google-genai": "^2.1.6",
    "@langchain/openai": "^1.1.3",
    "@langchain/pinecone": "^1.0.1",
    "@pinecone-database/pinecone": "^6.1.3",
    "@supabase/supabase-js": "^2.39.0",
    "@types/pdf-parse": "^1.1.5",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^1.1.1",
    "multer": "^1.4.5-lts.1",
    "p-limit": "^4.0.0",
    "pdf-parse": "^1.1.1",
    "pg": "^8.16.3",
    "uuid": "^13.0.0",
    "xlsx": "^0.18.5",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "babel-jest": "^30.2.0",
    "eslint": "^8.56.0",
    "form-data": "^4.0.5",
    "jest": "^30.2.0",
    "node-fetch": "^2.7.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
